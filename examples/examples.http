###############################################################################
# 23. RETRY LOGIC AND ERROR RECOVERY
###############################################################################

### Request with retry logic
@name retryExample
GET https://httpbin.org/status/503

< {
  if (!getVar('retryCount')) {
    setVar('retryCount', 0);
  }
}

> {
  if (response.status >= 500) {
    const retryCount = parseInt(getVar('retryCount') || '0');

    if (retryCount < 3) {
      setVar('retryCount', retryCount + 1);
      console.warn(`Request failed (attempt ${retryCount + 1}/3). Retry needed.`);
      setVar('shouldRetry', true);
    } else {
      console.error('Max retries reached. Request failed permanently.');
      setVar('shouldRetry', false);
    }
  } else {
    console.log('Request succeeded!');
    setVar('retryCount', 0);
  }
}

