###############################################################################
# 19. RESPONSE DATA EXTRACTION & MANIPULATION
###############################################################################

### Extract and transform data
@name dataExtraction
GET https://httpbin.org/json

> {
  // Extract nested data
  const data = response.json;

  // Store extracted values
  if (data.slideshow) {
    setVar('title', data.slideshow.title);
    setVar('author', data.slideshow.author);
    setVar('slides', JSON.stringify(data.slideshow.slides));
  }

  // Transform and store
  setVar('dataSize', JSON.stringify(data).length);
  setVar('responseTime', Date.now() - getVar('requestStartTime') || 0);

  console.log('Extracted:', {
    title: getVar('title'),
    author: getVar('author'),
    size: getVar('dataSize') + ' bytes'
  });
}

