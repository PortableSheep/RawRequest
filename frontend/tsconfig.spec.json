/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/spec",
    "moduleResolution": "bundler",
    "types": [
      "jest",
      "node"
    ],
    "paths": {
      "@wailsjs/*": ["wailsjs/*"],
      "@angular/cdk/scrolling": ["node_modules/@angular/cdk/types/scrolling.d.ts"]
    }
  },
  "include": [
    "src/**/*.spec.ts",
    "src/**/*.d.ts"
  ]
}
